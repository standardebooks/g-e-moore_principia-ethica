		<dc:title id="title">Principia Ethica</dc:title>
		<meta property="file-as" refines="#title">Principia Ethica</meta>
		<dc:subject id="subject-1">Ethics</dc:subject>
		<meta property="authority" refines="#subject-1">LCSH</meta>
